move = (record, type) => {
    const { dispatch } = this.props;
    dispatch({
      type: 'LtQuestion/move',
      payload: record,
      direction: type,
      callback: response => {
        if (response.flg === 'error') {
          notification.error({
            message: formatMessage({ id: 'global.tip' }),
            description: response.msg ? response.msg : formatMessage({ id: 'global.fail' }),
            duration: 3,
          });
        } else {
          notification.success({
            message: formatMessage({ id: 'global.tip' }),
            description: response.msg ? response.msg : formatMessage({ id: 'global.save.success' }),
            duration: 3,
          });
          this.onRefresh();
        }
      },
    });
  };